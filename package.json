{
  "name": "workcation",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css -o public/index.css",
    "build:js": "react-scripts build",
    "watch:css": "postcss src/styles/main.css -o public/index.css -w",
    "start": "yarn build:css && react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.6.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0"
  }
}
